<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(delivery) -%>">
  <span class='title'>
    <%= delivery.entity %>

  </span>
  <span class='actions'>

    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_deliveries_admin_delivery_path(delivery),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.deliveries_admin_delivery_path(delivery),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => delivery.entity),
        :method => :delete %>
  </span>
</li>